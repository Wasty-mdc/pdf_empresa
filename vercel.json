{
  "version": 2,
  "name": "pdf-empresa-informe-tecnico",
  "builds": [
    {
      "src": "informe/**",
      "use": "@vercel/static"
    },
    {
      "src": "diagram/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/informe/index.html"
    },
    {
      "src": "/informe/(.*)",
      "dest": "/informe/$1"
    },
    {
      "src": "/diagram/(.*)",
      "dest": "/diagram/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/informe/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}