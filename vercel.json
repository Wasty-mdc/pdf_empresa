{
  "version": 2,
  "name": "pdf-empresa-informe-tecnico",
  "rewrites": [
    {
      "source": "/",
      "destination": "/informe/index.html"
    },
    {
      "source": "/informe/(.*)",
      "destination": "/informe/$1"
    },
    {
      "source": "/diagram/(.*)",
      "destination": "/diagram/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/informe/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}